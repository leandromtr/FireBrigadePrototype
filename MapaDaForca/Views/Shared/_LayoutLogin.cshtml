<!DOCTYPE html>
<html lang="pt-pt">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0,minimal-ui">
    <title>Mapa da Força</title>
    <meta content="Mapa da Força" name="description">
    <meta content="Hexagon" name="author">
    <link rel="shortcut icon" href="/images/favicon.ico">
    <link href="/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="/css/icons.css" rel="stylesheet" type="text/css">
    <link href="/css/style.css" rel="stylesheet" type="text/css">
    @RenderSection("Styles", required: false)
</head>

<body>
    <!-- Begin page -->
    <div class="wrapper-page">
        <div class="card">
            <div class="card-body">
                <h3 class="text-center m-0"><img src="/images/logo.png" height="70" alt="logo"></h3>
                <div class="p-3">

                    @RenderBody()
                
                </div>
            </div>
        </div>

        <div class="m-t-40 text-center">
            @*<p>© Hexagon</p>*@
        </div>
    </div>
    <!-- jQuery  -->
    <script src="/jquery/jquery.min.js"></script>
    <script src="/twitter-bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/metisMenu/metisMenu.js"></script>
    <script src="/js/jquery.slimscroll.js"></script>
    <script src="/js/waves.min.js"></script>
    <script src="~/plugins/jquery-sparkline/jquery.sparkline.min.js"></script>
    <!-- App js -->
    <script src="/js/app.js"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>